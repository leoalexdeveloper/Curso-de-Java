package classe;

public class ValorVsReferencia {
	public static void main(String[] args) {
		double a = 2;
		double b = a;
		
		a++;
		b--;
		
		System.out.println(a + " " + b);
		
		Data d1 = new Data(1,6,2022);
		Data d2 = d1;
		
		d1.mes = 31;
		d2.mes = 12;
		
		System.out.printf("Data de d1 %d: ", d1.obterDataFormatada());
		System.out.printf("Datas de d2 %d: ", d2.obterDataFormatada());
		
	}
}
