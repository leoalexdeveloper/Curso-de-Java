package classe;

public class Jantar {
	public static void main(String[] args) {
		Comida sanduiche = new Comida("Sanduiche", 0.200);
		Comida pao = new Comida("PÃ£o", 0.349);
		
		Pessoa pessoa1 = new Pessoa("Adalberto", 90);
		Pessoa pessoa2 = new Pessoa("Birrento", 80);
		
		pessoa1.comer(pao);
		pessoa1.comer(sanduiche);
		
		System.out.println(pessoa1.pesar());
		System.out.println(pesso2.pesar());
	}
}
