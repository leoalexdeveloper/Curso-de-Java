package classe;

import java.util.Scanner;

public class ProdutoTeste {
	public static void main(String[] args) {
		
		Produto p1 = new Produto();
		p1.nome = "NOtebook";
		p1.preco = 4356.89;
		p1.desconto = 0.25;
		
		var p2 = new Produto();
		p2.nome = "CAneta Preta";
		p2.preco = 2.0;
		p2.desconto = 0.49; 
		
		System.out.println(p1.nome);
	}
}
